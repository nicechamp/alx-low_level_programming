#include "dog.h"
#include <stdio.h>

/**
 * print_dog - a function that prints a struct dog.
 * @d: struct to initialize
 * Return: Nothing.
 */
void print_dog(struct dog *d)
{
	if (d == NULL)
		return;
	if (d->name == NULL||d->owner == NULL)
		printf("(nil)");
	printf("Name: %s\nAge: 0.6%f\nOwner: %s", d->name, d->age, d->owner);
}
